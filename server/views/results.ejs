<% include partials/header %>

	<title>Results Page</title>
</head>
<body>
	<h1>Results Page</h1>
	<table border = "2">
	  <tr>
	    <th>Name</th>
	    <th>snippet</th>
	    <th>Profile Picture</th>
	    <!-- <th>IMDB Link</th> -->
	  </tr>
	  <% data['items'].forEach(function(anybody){ %>
	  <tr>
	    <!-- <% var imdb_url = 'https://www.imdb.com/title/' + anybody['imdbID'] %> -->
	    <td><%= anybody['title'] %></td>
	    <td><%= anybody['snippet'] %></td>
	    <td><img src="<%= anybody['pagemap[0].cse_image'] %>" alt=""></td>
	    <!-- <td><a href="<%= imdb_url %>">View on IMDB</a></td> -->
	  </tr>
	  <% }) %>
	</table>
	<br/>
 	<a href="/">Search Again!</a>
</body>
</html>
